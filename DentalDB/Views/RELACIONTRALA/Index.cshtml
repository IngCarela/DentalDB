@model IEnumerable<DentalDB.Models.RELACIONTRALA>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MasterPrincipal.cshtml";
}

<h2>Listado</h2>

<p>
    @Html.ActionLink("Crear relacion", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Hora)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TRABAJO.Tratamiento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LABORATORIO.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ESTADOTRABAJO.Estado)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Hora)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TRABAJO.Tratamiento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LABORATORIO.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ESTADOTRABAJO.Estado)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.IdRelacion }) |
            @Html.ActionLink("Detalle", "Details", new { id=item.IdRelacion }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.IdRelacion })
        </td>
    </tr>
}

</table>
